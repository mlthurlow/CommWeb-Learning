    <div class="row">
        <div class="col">
            <input id="id_filter" name="filter" type="text" class="form-control">
        </div>
    </div>
    <div class="row">
        <div class="col">
            <div class="table-responsive">
                <table class="table table-striped table-bordered table-hover table-sm">
                    <thead>
                        <tr>
                            <th scope="row">Header</th>
